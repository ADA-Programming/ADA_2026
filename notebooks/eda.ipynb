    "   \"metadata\": {},\n",
    "   \"cell_type\": \"code\",\n",
    "   \"outputs\": [],\n",
    "   \"execution_count\": null,\n",
    "   \"source\": \"# pivot_table = songs.pivot_table(values='energy', index='valence_category', columns='release_year')\\n\"\n",
